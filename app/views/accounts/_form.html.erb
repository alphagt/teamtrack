<%= simple_form_for(@account) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :email %>
    <%= f.input :primary_admin %>
    <%= f.input :secondary_admin %>
    <%= f.input :active %>
    <%= f.input :termdate %>
    <%= f.input :userquota %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
